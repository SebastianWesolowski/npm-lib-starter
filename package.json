{
  "name": "npm-lib-starter",
  "version": "0.0.0-development",
  "scripts": {
    "start": "react-scripts start",
    "build:react": "react-scripts build",
    "build": "run-s compile rollup:build",
    "rollup:build": "rollup -c",
    "test": "jest --maxWorkers=25%",
    "test:ci": "jest --ci",
    "test:watch": "jest",
    "test:coverage": "run-s \"test -- --coverage\" test:clear",
    "test:clear": "rimraf ./reports/coverage && mv coverage ./reports/",
    "test:update": "jest --updateSnapshot",
    "test:changed": "jest --onlyChanged",
    "eject": "react-scripts eject",
    "storybook": "NODE_PATH=src start-storybook -p 6006 -c .storybook -s ./public",
    "build-storybook": "build-storybook -s public",
    "compile": "run-s compile:clear compile:buildJS compile:alias compile:copy",
    "compile:clear": "npm run build:clear",
    "compile:buildJS": "tsc --project ./tsconfig.compile.json --declaration",
    "compile:alias": "tsc-alias -p ./tsconfig.compile.json",
    "compile:copy": "copyfiles -u 1 \"./src/**/*.+(png|svg|scss)\"  distTS",
    "build:clear": "rimraf distTS",
    "semantic-release": "semantic-release --branches main",
    "lint": "run-s format:check lint:parallel",
    "lint:parallel": "run-p lint:check style:check:strict",
    "lint:check": "eslint \"./src/**/*.+(js|jsx|ts|tsx)\" --cache",
    "lint:check:strict": "cross-env NODE_ENV=strictRules npm run lint:check -- --report-unused-disable-directives",
    "lint:check:staged": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "run-s format \"lint:check -- --fix\" style:fix",
    "lint:fix:strict": "cross-env NODE_ENV=strictRules npm run lint:fix -- --report-unused-disable-directives",
    "format": "run-s format:check format:write",
    "format:check": "prettier --check \"./**/*.+(js|jsx|ts|tsx|json)\" --ignore-unknown",
    "format:write": "prettier --write \"./**/*.+(js|jsx|ts|tsx|json)\" --ignore-unknown",
    "format:check:staged": "prettier --check --ignore-unknown",
    "style:check": "stylelint \"./src/**/*.+(css|scss|sass)\" --cache",
    "style:check:staged": "stylelint \"*.+(css|scss|sass)\" --cache",
    "style:check:strict": "cross-env NODE_ENV=strictRules npm run style:check --",
    "style:fix": "run-s \"style:check -- --fix\"",
    "new": "hygen new component --section components",
    "prepare": "husky install",
    "quality:check:jscpd": "jscpd ./src/",
    "quality:check:unimported": "unimported",
    "husky:prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
    "husky:pre-commit": "lint-staged -c ./.husky/lint-staged.config.json",
    "husky:pre-push": "run-s lint quality:check:* build",
    "husky:commit-msg": "commitlint --edit"
  },
  "dependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "git-cz": "^4.9.0",
    "git-cz-husky": "^0.1.1",
    "husky": "^8.0.1",
    "jscpd": "^3.4.5",
    "lint-staged": "^13.0.3",
    "react-test-renderer": "^18.2.0",
    "unimported": "^1.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-syntax-flow": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@react-theming/storybook-addon": "^1.1.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/storybook-deployer": "^2.8.10",
    "@svgr/webpack": "^6.1.2",
    "@tailwindcss/forms": "^0.5.2",
    "@testing-library/dom": "^7.21.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.15",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.1.0",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.0.0",
    "copyfiles": "^2.4.1",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-storybook": "^0.5.5",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "hygen": "^6.1.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-refresh": "^0.11.0",
    "react-scripts": "5.0.0",
    "rollup": "^2.61.1",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.54.5",
    "sass-loader": "10",
    "semantic-release": "^18.0.1",
    "storybook-addon-outline": "^1.4.2",
    "storybook-addon-sass-postcss": "^0.1.3",
    "storybook-readme": "^5.0.9",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-tailwindcss": "^0.0.3",
    "stylelint-no-unresolved-module": "^2.0.0",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "tailwind-merge": "^1.6.0",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "tsc-alias": "^1.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "private": false,
  "main": "./lib/index.js",
  "module": "./lib/index.es.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/*",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "engines": {
    "node": ">=14.19.0"
  },
  "license": "MIT",
  "description": "npm-lib-starter React Component Library",
  "author": "Sebastian Wesolowski",
  "homepage": "https://github.com/SebastianWesolowski/npm-lib-starter",
  "bugs": {
    "url": "https://github.com/SebastianWesolowski/npm-lib-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianWesolowski/npm-lib-starter.git"
  },
  "transform": {
    "\\.tsx$": "<rootDir>/node_modules/babel-jest"
  }
}
